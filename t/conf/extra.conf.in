<Perl>
	push @INC, "@ServerRoot@/../blib/lib";
</Perl>

PerlModule T::Session
<Location /test>
	PerlAccessHandler T::Session->access_handler
</Location>

<Location /test/basic_handler>
  SetHandler perl-script
  PerlHandler T::Basic
</Location>

<Location /test/swit>
  SetHandler perl-script
  PerlHandler T::SWIT
</Location>

<Location /test/another/page>
  SetHandler perl-script
  PerlHandler T::SWIT
</Location>

<Location /test/ht_page>
  SetHandler perl-script
  PerlSetVar SWITTemplate @ServerRoot@/templates/htpage.tt
  PerlHandler T::HTPage
</Location>

<Location /test/res>
  SetHandler perl-script
  PerlHandler T::Res
</Location>

<Location /test/sess_page>
  SetHandler perl-script
  PerlSetVar SWITTemplate @ServerRoot@/templates/sess_page.tt
  PerlHandler T::SessPage
</Location>


